{
	"ecmaFeatures": {
		"modules": true
	},
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"jquery": true
	},
	"rules": {
		"camelcase": 0,  /* Avoid detecting 'opt_foobar' */
		"dot-notation": 0,
		"no-multi-spaces": 0,
		"no-undef": 2,
		"no-underscore-dangle": 0,
		"no-unused-vars": 1,
		"no-use-before-define": 2,
		"semi-spacing": 0,
		"strict": 0,
		"quotes": [1, "single"]
	},
	"globals": {
		"p5": true,
		"PIXI": true
	}
}
/* vim: set filetype=javascript: */
